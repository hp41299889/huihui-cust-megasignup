// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Signup {
  id            Int     @id @default(autoincrement())
  name          String
  phone         String  @unique
  email         String
  childName     String
  introducer    String
  signupNumbers Int
  isVerify      Boolean
  isCheckin     Boolean
}
